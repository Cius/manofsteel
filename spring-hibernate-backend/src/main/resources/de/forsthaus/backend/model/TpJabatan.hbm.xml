<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="de.forsthaus.backend.model.TpJabatan" table="tp_jabatan">

		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment">
				<param name="sequence">tpjabatan_seq</param>
			</generator>
		</id>

		<version name="version" column="VERSION" type="integer" />

		<property name="nip" type="string">
			<column name="NIP" />
		</property>
		<property name="kodePejJab" type="string">
			<column name="KPEJJAB" />
		</property>
		<property name="numSkJabat" type="string">
			<column name="NSKJABAT" />
		</property>
		<property name="tglSkJabat" type="date">
			<column name="TSKJABAT" />
		</property>
		<property name="jnsJab" type="string">
			<column name="JNSJAB" />
		</property>
		<property name="kodeJab" type="string">
			<column name="KJAB" />
		</property>
		<property name="kodeEselon" type="string">
			<column name="KESELON" />
		</property>
		<property name="tmtjab" type="date">
			<column name="TMTJAB" />
		</property>
		<property name="sumpahJab" type="string">
			<column name="SJAB" />
		</property>
		<property name="namaJab" type="string">
			<column name="NJAB" />
		</property>
		<property name="namaJab1" type="string">
			<column name="NJAB1" />
		</property>
		<property name="kodeInsKer" type="string">
			<column name="KINSKER" />
		</property>
		<property name="kodeUnKer" type="string">
			<column name="KUNKER" />
		</property>
		<property name="kodeSatKer" type="string">
			<column name="KSATKER" />
		</property>
		<property name="tmtEselon" type="string">
			<column name="TMTESELON" />
		</property>
		<property name="tmtJab1" type="date">
			<column name="TMTJAB1" />
		</property>
		<property name="kodeJnsGuru" type="string">
			<column name="KJNSGURU" />
		</property>
		<property name="kodeInsInd" type="string">
			<column name="KINSIND" />
		</property>
		<property name="starec" type="string">
			<column name="STAREC" />
		</property>
		<property name="userEnt" type="string">
			<column name="USERENT" />
		</property>
		<property name="tglEnt" type="date">
			<column name="TGLENT" />
		</property>
		<property name="userEdt" type="string">
			<column name="USEREDT" />
		</property>
		<property name="tglEdt" type="date">
			<column name="TGLEDT" />
		</property>
	</class>
</hibernate-mapping>
