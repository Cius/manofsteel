<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="de.forsthaus.backend.model.TrKeluarga" table="tr_keluarga">

		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment">
				<param name="sequence">trkeluarga_seq</param></generator>
		</id>

		<version name="version" column="VERSION" type="integer" />

		<property name="nip" type="string">
			<column name="NIP" />
		</property>
		<property name="kHubKel" type="string">
			<column name="KHUBKEL" />
		</property>
		<property name="namaKel" type="string">
			<column name="NAMAKEL" />
		</property>
		<property name="tmLahir" type="string">
			<column name="TMLAHIR" />
		</property>
		<property name="tgLahir" type="string">
			<column name="TGLAHIR" />
		</property>
		<property name="tgKawin" type="string">
			<column name="TGKAWIN" />
		</property>
		<property name="kSex" type="string">
			<column name="KSEX" />
		</property>
		<property name="kTpu" type="string">
			<column name="KTPU" />
		</property>
		<property name="statun" type="string">
			<column name="STATUN" />
		</property>
		<property name="kKerja" type="string">
			<column name="KKERJA" />
		</property>
		<property name="alJalan1" type="string">
			<column name="ALJALAN1" />
		</property>
		<property name="alJalan2" type="string">
			<column name="ALJALAN2" />
		</property>
		<property name="alJalan3" type="string">
			<column name="ALJALAN3" />
		</property>
		<property name="alJalan4" type="string">
			<column name="ALJALAN4" />
		</property>
		<property name="starec" type="string">
			<column name="STAREC" />
		</property>
		<property name="nipIsMi" type="string">
			<column name="NIPISMI" />
		</property>
		<property name="tgMutasi" type="string">
			<column name="TGMUTASI" />
		</property>
		<property name="userEnt" type="string">
			<column name="USERENT" />
		</property>
		<property name="tgEnt" type="date">
			<column name="TGENT" />
		</property>
		<property name="userEdt" type="string">
			<column name="USEREDT" />
		</property>
		<property name="tgEdt" type="date">
			<column name="TGEDT" />
		</property>
	</class>
</hibernate-mapping>
