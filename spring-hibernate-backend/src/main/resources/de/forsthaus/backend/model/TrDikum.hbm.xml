<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="de.forsthaus.backend.model.TrDikum" table="tr_dikum">

		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment">
				<param name="sequence">trdikum_seq</param></generator>
		</id>

		<version name="version" column="VERSION" type="integer" />

		<property name="nip" type="string">
			<column name="NIP" />
		</property>
		<property name="kodeTpu" type="string">
			<column name="KTPU" />
		</property>
		<property name="kodeFak" type="string">
			<column name="KFAK" />
		</property>
		<property name="kodeJur" type="string">
			<column name="KJUR" />
		</property>
		<property name="numJur" type="string">
			<column name="NJUR" />
		</property>
		<property name="numSek" type="string">
			<column name="NSEK" />
		</property>
		<property name="tempat" type="string">
			<column name="TEMPAT" />
		</property>
		<property name="numKepSek" type="string">
			<column name="NKEPSEK" />
		</property>
		<property name="numSttb" type="string">
			<column name="NSTTB" />
		</property>
		<property name="tglSttb" type="string">
			<column name="TSTTB" />
		</property>
		<property name="userEnt" type="string">
			<column name="USERENT" />
		</property>
		<property name="tglEnt" type="date">
			<column name="TGLENT" />
		</property>
		<property name="userEdt" type="string">
			<column name="USEREDT" />
		</property>
		<property name="tglEdt" type="date">
			<column name="TGLEDT" />
		</property>
	</class>
</hibernate-mapping>
