<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="de.forsthaus.backend.model.TrOrganisasi" table="tr_organisasi">

		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment">
				<param name="sequence">trorganisasi_seq</param>
			</generator>
		</id>

		<version name="version" column="VERSION" type="integer" />

		<property name="nip" type="string">
			<column name="NIP" />
		</property>
		<property name="kodeJnsOrg" type="string">
			<column name="KJNSORG" />
		</property>
		<property name="numOrganisasi" type="string">
			<column name="NORGANISASI" />
		</property>
		<property name="jabOrg" type="string">
			<column name="JABORG" />
		</property>
		<property name="pimpinan" type="string">
			<column name="PIMPINAN" />
		</property>
		<property name="tempat" type="string">
			<column name="TEMPAT" />
		</property>
		<property name="tglMulai" type="string">
			<column name="TMULAI" />
		</property>
		<property name="tglAkhir" type="string">
			<column name="TAKHIR" />
		</property>
		<property name="userEnt" type="string">
			<column name="USERENT" />
		</property>
		<property name="tglEnt" type="date">
			<column name="TGLENT" />
		</property>
		<property name="userEdt" type="string">
			<column name="USEREDT" />
		</property>
		<property name="tglEdt" type="date">
			<column name="TGLEDT" />
		</property>
	</class>
</hibernate-mapping>
