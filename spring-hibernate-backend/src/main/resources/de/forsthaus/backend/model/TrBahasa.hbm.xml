<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="de.forsthaus.backend.model.TrBahasa" table="tr_bahasa">

		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment">
				<param name="sequence">trbahasa_seq</param></generator>
		</id>

		<version name="version" column="VERSION" type="integer" />

		<property name="nip" type="string">
			<column name="NIP" />
		</property>
		<property name="nBahasa" type="string">
			<column name="NBAHASA" />
		</property>
		<property name="kBahasa" type="string">
			<column name="KBAHASA" />
		</property>
		<property name="jBahasa" type="string">
			<column name="JBAHASA" />
		</property>
		<property name="userEnt" type="string">
			<column name="USERENT" />
		</property>
		<property name="tgEnt" type="date">
			<column name="TGENT" />
		</property>
		<property name="userEdt" type="string">
			<column name="USEREDT" />
		</property>
		<property name="tgEdt" type="date">
			<column name="TGEDT" />
		</property>
	</class>
</hibernate-mapping>
