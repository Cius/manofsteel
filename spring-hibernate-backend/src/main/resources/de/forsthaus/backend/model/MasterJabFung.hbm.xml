<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="de.forsthaus.backend.model.MasterJabFung" table="mst_jabfung">

		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment">
				<param name="sequence">mstjabfung_seq</param></generator>
		</id>

		<version name="version" column="VERSION" type="integer" />

		<property name="kodeJabFung" type="string">
			<column name="KJABFUNG" />
		</property>
		<property name="namaJabFung" type="string">
			<column name="NJABFUNG" />
		</property>
		<property name="golAwal" type="string">
			<column name="GAWAL" />
		</property>
		<property name="golAkhir" type="string">
			<column name="GAKHIR" />
		</property>
		<property name="tunJab" type="string">
			<column name="TUNJAB" />
		</property>
		<property name="bup" type="string">
			<column name="BUP" />
		</property>
		<property name="akredit" type="string">
			<column name="AKREDIT" />
		</property>
		<property name="ktpu" type="string">
			<column name="KTPU" />
		</property>
		<property name="status" type="string">
			<column name="STATUS" />
		</property>
	</class>
</hibernate-mapping>
